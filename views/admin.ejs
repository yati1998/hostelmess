<body class="tm-gray-bg">
	<!-- Header -->
	<div class="tm-header">
		<div class="container">
			<div class="row">
				<div class="col-lg-6 col-md-4 col-sm-3 tm-site-name-container">
					<a href="#" class="tm-site-name">Admin</a>
				</div>
				<div class="col-lg-6 col-md-8 col-sm-9">
					<div class="mobile-menu-icon">
						<i class="fa fa-bars"></i>
					</div>
					<nav class="tm-nav">
						<ul>
							<li><a href="admin">Issues</a></li>
							<li><a href="mealcounts">Meal Count</a></li>
							<li><a href="logout">Logout</a></li>
						</ul>
					</nav>
				</div>
			</div>
		</div>
	</div>
	<center>
		<form action="admin" method="post">
			<input type="submit" value="Show Issues"/>
		</form>
	</center>
	<%if(users!=undefined) {%>
	<table id="myTable" width="100%" align="center" class="thetab">
      <thead>
				<tr>
        	<th>Redg No</th>
        	<th>Type</th>
        	<th>Description</th>
        	<th>Date</th>
      	</tr>
			</thead>
			<tbody>
      <% for(var i=0; i < users.length; i++) { %>
      <tr>
       <td><%= users[i].regno %></td>
       <td><%= users[i].type %></td>
       <td><%= users[i].description %></td>
       <td><%= users[i].date %></td>
      </tr>
      <% } } %>
		</tbody>
</table>


 </body>

